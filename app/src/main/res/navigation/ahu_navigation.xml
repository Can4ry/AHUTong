<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ahu_navigation"
    app:startDestination="@id/home_fragment">

    <fragment
        android:id="@+id/home_fragment"
        android:name="com.ahu.ahutong.ui.page.HomeFragment"
        tools:layout="@layout/fragment_home">

        <action
            android:id="@+id/action_home_fragment_to_developer_fragment"
            app:destination="@id/developer_fragment" />

        <action
            android:id="@+id/action_home_fragment_to_business_fragment"
            app:destination="@id/business_fragment" />
    </fragment>

    <fragment
        android:id="@+id/discovery_fragment"
        android:name="com.ahu.ahutong.ui.page.DiscoveryFragment"
        tools:layout="@layout/fragment_discovery" />

    <fragment
        android:id="@+id/developer_fragment"
        android:name="com.ahu.ahutong.ui.page.DeveloperFragment"
        tools:layout="@layout/fragment_developer" />
    <fragment
        android:id="@+id/business_fragment"
        android:name="com.ahu.ahutong.ui.page.BusinessFragment"
        tools:layout="@layout/fragment_business" />
    <fragment
        android:id="@+id/login_fragment"
        android:name="com.ahu.ahutong.ui.page.LoginFragment"
        tools:layout="@layout/fragment_login" />
    <fragment
        android:id="@+id/setting_fragment"
        android:name="com.ahu.ahutong.ui.page.SettingFragment" />


    <fragment
        android:id="@+id/teldirectory_fragment"
        android:name="com.ahu.ahutong.ui.page.TelDirectoryFragment"
        tools:layout="@layout/fragment_teldirectory" />
    <fragment
        android:id="@+id/exam_fragment"
        android:name="com.ahu.ahutong.ui.page.ExamFragment"
        tools:layout="@layout/fragment_exam" />
    <fragment
        android:id="@+id/gradle_fragment"
        android:name="com.ahu.ahutong.ui.page.GradeFragment"
        tools:layout="@layout/fragment_grade" />
    <fragment
        android:id="@+id/about_fragment"
        android:name="com.ahu.ahutong.ui.page.AboutFragment"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/change_theme_fragment"
        android:name="com.ahu.ahutong.ui.page.ChangeThemeFragment"
        tools:layout="@layout/fragment_change_theme" />
    <fragment
        android:id="@+id/course_fragment"
        android:name="com.ahu.ahutong.ui.page.CourseFragment"
        tools:layout="@layout/fragment_course" />
    <fragment
        android:id="@+id/license_fragment"
        android:name="com.ahu.ahutong.ui.page.LicenseFragment"
        tools:layout="@layout/fragment_license" />
</navigation>